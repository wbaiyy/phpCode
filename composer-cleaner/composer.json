{
    "name": "Wbaiyy/composer-cleaner",
    "type": "composer-plugin",
    "description": "清除vendor下无用文件",
    "keywords": ["composer", "clean", "cleaner"],
    "license": "MIT",
    "authors": [
        {
            "name": "mashanling",
            "email": "mashanling@Wbaiyy.com"
        }
    ],
    "autoload": {
        "classmap": ["src"]
    },
    "autoload-dev": {
        "psr-4": {
            "Wbaiyy\\Tests\\": ["tests"]
        }
    },
    "require": {
        "php": ">=5.6",
        "composer-plugin-api": "^1.1"
    },
    "require-dev": {
        "composer/composer": "^1.6",
        "phpunit/php-token-stream": "<2",
        "phpunit/phpunit": "^5"
    },
    "extra": {
        "class": "Wbaiyy\\ComposerCleaner\\Plugin"
    }
}
